{
  "name": "opensearch-document-search",
  "version": "1.0.0",
  "description": "A web application for searching documents using OpenSearch",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "keywords": [
    "opensearch",
    "search",
    "documents",
    "webapp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@opensearch-project/opensearch": "^2.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "node-rake": "^1.0.1",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.36.0"
  }
}
